<control>
label_cc7=Master vol
label_cc10=Master pan (disabled)

label_cc30=Bobobo bass vol
label_cc31=Bobobo bass pan
label_cc32=Bobobo bass tune
label_cc33=Bobobo bass mute

label_cc35=Bobobo L tenor vol
label_cc36=Bobobo L tenor pan
label_cc37=Bobobo L tenor tune
label_cc38=Bobobo L tenor mute

label_cc40=Bobobo S tenor vol
label_cc41=Bobobo S tenor pan
label_cc42=Bobobo S tenor tune
label_cc43=Bobobo S tenor mute

label_cc45=Bobobo L treble vol
label_cc46=Bobobo L treble pan
label_cc47=Bobobo L treble tune
label_cc48=Bobobo L treble mute

label_cc50=Bobobo S treble vol
label_cc51=Bobobo S treble pan
