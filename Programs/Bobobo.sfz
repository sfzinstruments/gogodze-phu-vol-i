label_cc53=Bobobo S treble mute

label_cc55=Bobobo all tune spread

set_cc7=100
set_cc10=63

set_cc30=100
set_cc31=63
set_cc32=63

set_cc35=100
set_cc36=33
set_cc37=63

set_cc40=100
set_cc41=0
set_cc42=63

set_cc45=100
set_cc46=93
set_cc47=63

set_cc50=100
set_cc51=127
set_cc52=63

<global>
loop_mode=one_shot
off_mode=normal
tune=-1200 //Everything is tuned down an octave, then tuned back up with its own tune param
pan=-100 //Similar for pan - hard left, brought to proper position with param
pan_oncc10=0 //Disable master pan
amplitude_oncc7=100
ampeg_attack=0.001
ampeg_decay=0.7
ampeg_sustain=100

<master>
amplitude_cc30=100
pan_cc31=200
tune_cc32=2400
tune_cc55=-500
offset_cc33=3000
ampeg_sustain_oncc33=-100
#include "mappings/bobobo_bass.sfz"

